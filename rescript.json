{
  "name": "rescript-ic10-compiler",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "examples",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "suffix": ".res.js",
  "bs-dependencies": ["@rescript/core"],
  "gentypeconfig": {
    "module": "esmodule",
    "moduleResolution": "node",
    "generatedFileExtension": ".gen.ts"
  },
  "bsc-flags": ["-open RescriptCore", "-open IC10"]
}

